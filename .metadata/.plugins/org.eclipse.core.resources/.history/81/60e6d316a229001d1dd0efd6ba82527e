@Setter
@ToString
public class CrewPostDTO {
    private Long postNo;
    private Crew crew;
    private Member member;
    private String category;
    private String title;
    private String content;
    private String memberList;
    private String uploadImg;
    private LocalDateTime wDate;
    private Integer hit;
}
