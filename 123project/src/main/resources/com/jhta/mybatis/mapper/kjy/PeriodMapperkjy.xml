<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.mapper.kjy.PeriodMapperkjy">
	<select id="list" resultType="string" parameterType="hashmap">
		select peseason from period 
		<![CDATA[ where pestart <= #{day} and #{day} <= peend and aid=#{aid}]]>
	</select>
</mapper>